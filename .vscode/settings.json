  "editor.tokenColorCustomizations": {
    "comments": "#6A9955",
    "strings": "#CE9178",
    "keywords": "#C586C0",
    "numbers": "#B5CEA8",
    "types": "#4EC9B0",
    "functions": "#DCDCAA",
    "variables": "#9CDCFE",
    "textMateRules": [
      // ═══════════════════════════════════════════════════════════════
      // COMMENTAIRES - Vert standard (style VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": ["comment", "comment.line", "comment.block", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#6A9955"
        }
      },
      {
        "scope": "comment.block.documentation",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#6A9955"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // MOTS-CLÉS - VIOLET/MAGENTA (standard VS Code)
      // import, export, if, else, return, throw, try, catch, etc.
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "keyword",
          "keyword.control",
          "keyword.control.import",
          "keyword.control.export",
          "keyword.control.from",
          "keyword.control.as",
          "keyword.control.default",
          "keyword.control.conditional",
          "keyword.control.loop",
          "keyword.control.flow",
          "keyword.control.trycatch",
          "keyword.control.switch"
        ],
        "settings": {
          "foreground": "#C586C0"
        }
      },
      {
        "scope": [
          "keyword.operator.new",
          "keyword.operator.expression",
          "keyword.operator.typeof",
          "keyword.operator.instanceof",
          "keyword.operator.delete",
          "keyword.operator.void",
          "keyword.operator.spread",
          "keyword.operator.rest"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // STORAGE - BLEU (const, let, var, function, class, interface)
      // Standard VS Code Dark+
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "storage.type",
          "storage.type.function",
          "storage.type.class",
          "storage.type.interface",
          "storage.type.type",
          "storage.type.enum",
          "storage.type.namespace"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": [
          "storage.modifier",
          "storage.modifier.async",
          "storage.modifier.static",
          "storage.modifier.readonly",
          "storage.modifier.public",
          "storage.modifier.private",
          "storage.modifier.protected"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // CHAÎNES - ORANGE/SAUMON (standard VS Code Dark+)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "string",
          "string.quoted",
          "string.quoted.single",
          "string.quoted.double",
          "string.template",
          "string.interpolated"
        ],
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": ["string.regexp", "string.regexp.js", "string.regexp.ts"],
        "settings": {
          "foreground": "#D16969"
        }
      },
      {
        "scope": "punctuation.definition.string",
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": [
          "string.template punctuation.definition.template-expression",
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // NOMBRES ET CONSTANTES - VERT CLAIR (standard VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.numeric.binary"
        ],
        "settings": {
          "foreground": "#B5CEA8"
        }
      },
      {
        "scope": [
          "constant.language",
          "constant.language.boolean",
          "constant.language.boolean.true",
          "constant.language.boolean.false",
          "constant.language.null",
          "constant.language.undefined",
          "constant.language.nan",
          "constant.language.infinity"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": "constant.character.escape",
        "settings": {
          "foreground": "#D7BA7D"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // FONCTIONS - JAUNE (standard VS Code Dark+)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "entity.name.function",
          "entity.name.function.member",
          "support.function",
          "meta.function-call entity.name.function"
        ],
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      {
        "scope": ["meta.function-call", "variable.function"],
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      {
        "scope": "support.function.builtin",
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      {
        "scope": ["entity.name.function.method", "meta.method.declaration entity.name.function"],
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // TYPES, CLASSES, INTERFACES - CYAN/TEAL (standard VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "entity.name.type",
          "entity.name.type.class",
          "entity.name.type.interface",
          "entity.name.type.enum",
          "entity.name.type.alias",
          "entity.name.type.module"
        ],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "scope": ["entity.name.class", "entity.other.inherited-class"],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "scope": [
          "support.type",
          "support.type.builtin",
          "support.type.primitive",
          "support.class",
          "support.class.builtin"
        ],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "scope": ["entity.name.type.parameter", "meta.type.parameters entity.name.type"],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // VARIABLES - BLEU CLAIR (standard VS Code Dark+)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": ["variable", "variable.other", "variable.other.readwrite", "variable.other.local"],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": ["variable.parameter", "variable.parameter.function"],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": ["variable.other.constant", "variable.other.enummember"],
        "settings": {
          "foreground": "#4FC1FF"
        }
      },
      {
        "scope": [
          "variable.language",
          "variable.language.this",
          "variable.language.super",
          "variable.language.self"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // PROPRIÉTÉS D'OBJETS - Bleu clair
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "variable.other.property",
          "variable.other.object.property",
          "meta.object-literal.key",
          "support.type.property-name",
          "entity.name.property"
        ],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": "meta.object.member variable.other.readwrite",
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // OPÉRATEURS - BLANC/GRIS (standard VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "keyword.operator",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.operator.comparison",
          "keyword.operator.relational",
          "keyword.operator.assignment",
          "keyword.operator.ternary",
          "keyword.operator.nullish"
        ],
        "settings": {
          "foreground": "#D4D4D4"
        }
      },
      {
        "scope": ["keyword.operator.type", "keyword.operator.type.annotation"],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // PONCTUATION - BLANC/GRIS (standard VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "punctuation",
          "punctuation.separator",
          "punctuation.separator.comma",
          "punctuation.separator.parameter",
          "punctuation.terminator",
          "punctuation.terminator.statement",
          "punctuation.accessor",
          "meta.brace.round",
          "meta.brace.square",
          "meta.brace.curly"
        ],
        "settings": {
          "foreground": "#D4D4D4"
        }
      },
      {
        "scope": [
          "punctuation.definition.block",
          "punctuation.section.block",
          "punctuation.section.braces",
          "punctuation.section.brackets",
          "punctuation.section.parens"
        ],
        "settings": {
          "foreground": "#D4D4D4"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // ARROW FUNCTIONS - BLEU (standard VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "storage.type.function.arrow",
          "keyword.operator.arrow",
          "punctuation.definition.arrow"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // TAGS HTML/JSX - BLEU (standard VS Code)
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "entity.name.tag",
          "entity.name.tag.html",
          "entity.name.tag.js",
          "entity.name.tag.tsx",
          "support.class.component"
        ],
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": [
          "punctuation.definition.tag",
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end"
        ],
        "settings": {
          "foreground": "#808080"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name",
          "entity.other.attribute-name.html",
          "entity.other.attribute-name.js"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#FFCB6B"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // JSON KEYS
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": ["support.type.property-name.json", "string.json support.type.property-name"],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // MARKDOWN
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": ["markup.heading", "markup.heading.markdown", "entity.name.section.markdown"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#82CFFF"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#F5A962"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#C792EA"
        }
      },
      {
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#98D9A0"
        }
      },
      {
        "scope": "markup.underline.link",
        "settings": {
          "foreground": "#4FC1E9"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // DECORATORS / ANNOTATIONS
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "meta.decorator",
          "meta.decorator punctuation.decorator",
          "punctuation.decorator"
        ],
        "settings": {
          "foreground": "#FFCB6B"
        }
      },
      {
        "scope": "entity.name.function.decorator",
        "settings": {
          "foreground": "#FFCB6B"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // ERREURS ET AVERTISSEMENTS
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
        "settings": {
          "fontStyle": "strikethrough",
          "foreground": "#F07178"
        }
      },
      // ═══════════════════════════════════════════════════════════════
      // CSS/SCSS
      // ═══════════════════════════════════════════════════════════════
      {
        "scope": [
          "entity.other.attribute-name.class.css",
          "entity.other.attribute-name.class.scss"
        ],
        "settings": {
          "foreground": "#FFCB6B"
        }
      },
      {
        "scope": ["entity.other.attribute-name.id.css", "entity.other.attribute-name.id.scss"],
        "settings": {
          "foreground": "#F07178"
        }
      },
      {
        "scope": ["support.type.property-name.css", "support.type.property-name.scss"],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": ["support.constant.property-value.css", "support.constant.property-value.scss"],
        "settings": {
          "foreground": "#BDD4E0"
        }
      },
      {
        "scope": ["constant.numeric.css", "keyword.other.unit.css"],
        "settings": {
          "foreground": "#F5A962"
        }
      }
    ]
  },
  "workbench.tree.indent": 20,
  "workbench.list.smoothScrolling": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "breadcrumbs.enabled": true
}
